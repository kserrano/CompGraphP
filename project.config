[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/kserrano
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/kserrano
	label-Verified = -1..+1 group user/kserrano
	push = group user/kserrano
	read = group user/kserrano
	rebase = group user/kserrano
	removeReviewer = group user/kserrano
	submit = group user/kserrano
[access "refs/heads/*"]
	create = group user/kserrano
	push = +force group user/kserrano
	pushMerge = group user/kserrano
	read = group user/kserrano
[access "refs/tags/*"]
	push = group user/kserrano
	push = +force group user/kserrano
